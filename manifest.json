{
	"manifest_version": 2,
	"name": "Remove From Feed for YouTube",
	"description": "Buttons to one-click remove videos from your YouTube subscription feed or remove all watched wideos",
	"version": "0.0.2",
	"options_page": "options.html",
	"permissions": ["http://www.youtube.com/*", "https://www.youtube.com/*"],
	"icons": {
		"16": "icon_16.png",
		"48": "icon_48.png",
		"128": "icon_128.png"
	},
	"web_accessible_resources":["close_16_r8.png"],
	"content_scripts": [
		{
			"js": ["includes/jquery.js", "includes/userscript.user.js"],
			"matches": ["http://www.youtube.com/*", "https://www.youtube.com/*"],
			"run_at": "document_end"
		}
	]
}